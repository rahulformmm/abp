pipeline {
    agent any
    stages {
      stage ('Hello') {
        steps {
          sh 'echo Hello World'
        }
       }
       stage ('Middle') {
        steps {
          sh 'echo Middle.'
        }
       }
        stage ('Bye Bye') {
        steps {
          sh 'echo Bye Bye.'
        }
       }
  }
}

  
  
